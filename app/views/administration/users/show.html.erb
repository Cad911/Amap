		 <div class="page-header">
   			<h1> <%= @title_user %> </h1>
  		</div>
   
			

			<div class="well">
			<p> id current <%= current_user.id %></p>
			<p id="notice"><%= notice %></p>
				<p>
				  <b>Id:</b>
				  <%= @user.id %>
				</p>
				
				<p>
				  <b>Entite:</b>
